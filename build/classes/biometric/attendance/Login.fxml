<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import java.net.URL?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="371.0" prefWidth="349.0" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="biometric.attendance.LoginControl">
    <stylesheets>
        <URL value="@logincss.css" />
    </stylesheets>
   <children>
      <Pane fx:id="adminbg" prefHeight="371.0" prefWidth="349.0">
         <children>
            <JFXTextField fx:id="Admin_id" focusColor="#002b80" layoutX="12.0" layoutY="85.0" prefHeight="28.0" prefWidth="326.0" promptText="Login ID" unFocusColor="BLACK" />
            <JFXPasswordField fx:id="Amin_Password" focusColor="#002cae" layoutX="12.0" layoutY="150.0" onKeyPressed="#LoginKey" prefHeight="25.0" prefWidth="326.0" promptText="Password" unFocusColor="BLACK" />
            <JFXButton fx:id="Loginbtn" buttonType="RAISED" layoutX="95.0" layoutY="230.0" onAction="#login" prefHeight="48.0" prefWidth="159.0" ripplerFill="#ff0707" text="LOGIN" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="22.0" />
               </font>
            </JFXButton>
            <Label fx:id="loginstatus" layoutX="12.0" layoutY="23.0" prefHeight="48.0" prefWidth="302.0" textFill="#da0909">
               <font>
                  <Font name="System Bold Italic" size="12.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </children>
</AnchorPane>
